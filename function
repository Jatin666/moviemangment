def movie():
    userinput1=input("enter the movie")
    userinput2 = input("enter the release date")
    userinput3 = input("enter the rating")
    return [userinput1,userinput2,userinput3]
def add():
    movielist=[]
    movielist.extend(movie())
    return movielist








































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































